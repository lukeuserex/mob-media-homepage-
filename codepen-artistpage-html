<script>
function toggleDiv(divId, imgElement) {
  var div = document.getElementById(divId);
  if (div.style.display === "none") {
    div.style.display = "block";
  } else {
    div.style.display = "none";
  }

  // Get the image URL from the data-src attribute of the img element
  var src = imgElement.getAttribute('data-src');
  // Set the src attribute of the icons img element to the image URL
  document.getElementById(divId + '-img').src = src;
}
</script>


